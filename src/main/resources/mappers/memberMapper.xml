<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.Mapper">
    <select id="select2" resultType="controller.Member">
        select * from member
    </select>
    <insert id="insert2" parameterType="controller.Member">
        insert into
            member(name, age)
            values(#{name}, #{age})
    </insert>

</mapper>